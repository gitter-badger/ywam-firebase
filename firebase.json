{
  "database": {
    "rules": "database.rules.bolt"
  },
  "hosting": {
    "public": "dist",
    "headers": [
      {
        "source": "**/*.@(js)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000"
          }
        ]
      }
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}
